{
  "compilerOptions": {
    "noImplicitAny": true,
    "outDir": "./dist",
    "sourceMap": false,
    "esModuleInterop": true,
    "lib": ["DOM", "ESNext"],
    "target": "ES6",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "include": ["src/**/*"],
  "exclude": ["dist/**/*"]
}
